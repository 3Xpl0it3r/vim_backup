set timeoutlen=50

" 加载插件
source ~/.vim/config.d/plugin.vim

" vim相关配置
source ~/.vim/config.d/common.vim

"which-key
source ~/.vim/config.d/which-key.vim


" 目录树插件
" source ~/.vim/config.d/defx.vim
source ~/.vim/config.d/netrd.vim
"coc 配置
source ~/.vim/config.d/coc.vim
"
"floaterm配置
source ~/.vim/config.d/floaterm.vim

"键盘映射
source ~/.vim/config.d/map.vim

" rainbow
source ~/.vim/config.d/rainbow.vim

" indentline
source ~/.vim/config.d/indent.vim

" aireline config
source ~/.vim/config.d/aireline.vim

" markdown预览
source ~/.vim/config.d/markdown.vim

" vista
source ~/.vim/config.d/vista.vim

